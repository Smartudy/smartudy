<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_marginTop="@dimen/home_content_top_margin"
    android:orientation="vertical"
    app:layout_behavior="@string/appbar_scrolling_view_behavior">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" android:baselineAligned="false">

        <RelativeLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center">

            <ImageButton
                android:id="@+id/ImageButton0"
                android:layout_width="@dimen/home_content_image_width"
                android:layout_height="@dimen/home_content_image_height"
                android:src="@drawable/one" />

            <TextView
                android:layout_width="@dimen/home_content_image_width"
                android:layout_height="@dimen/home_content_image_height"
                android:layout_alignBottom="@id/ImageButton0"
                android:layout_alignLeft="@id/ImageButton0"
                android:layout_alignRight="@id/ImageButton0"
                android:layout_alignTop="@id/ImageButton0"
                android:paddingBottom="@dimen/home_content_image_padding_bottom"
                android:gravity="bottom|center_horizontal"
                android:textSize="@dimen/home_text_size"
                android:textColor="@color/Home_Text_Color"
                android:fontFamily="@font/nanumsquareb"
                android:text="인문"/>

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center">

            <ImageButton
                android:id="@+id/ImageButton1"
                android:layout_width="@dimen/home_content_image_width"
                android:layout_height="@dimen/home_content_image_height"
                android:src="@drawable/two" />
            <TextView
                android:layout_width="@dimen/home_content_image_width"
                android:layout_height="@dimen/home_content_image_height"
                android:layout_alignBottom="@id/ImageButton1"
                android:layout_alignLeft="@id/ImageButton1"
                android:layout_alignRight="@id/ImageButton1"
                android:layout_alignTop="@id/ImageButton1"
                android:paddingBottom="@dimen/home_content_image_padding_bottom"
                android:gravity="bottom|center_horizontal"
                android:textSize="@dimen/home_text_size"
                android:textColor="@color/Home_Text_Color"
                android:fontFamily="@font/nanumsquareb"
                android:text="사회"/>

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center">

            <ImageButton
                android:id="@+id/ImageButton2"
                android:layout_width="@dimen/home_content_image_width"
                android:layout_height="@dimen/home_content_image_height"
                android:src="@drawable/three" />
            <TextView
                android:layout_width="@dimen/home_content_image_width"
                android:layout_height="@dimen/home_content_image_height"
                android:layout_alignBottom="@id/ImageButton2"
                android:layout_alignLeft="@id/ImageButton2"
                android:layout_alignRight="@id/ImageButton2"
                android:layout_alignTop="@id/ImageButton2"
                android:paddingBottom="@dimen/home_content_image_padding_bottom"
                android:gravity="bottom|center_horizontal"
                android:textSize="@dimen/home_text_size"
                android:textColor="@color/Home_Text_Color"
                android:fontFamily="@font/nanumsquareb"
                android:text="교육"/>

        </RelativeLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <RelativeLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_weight="1"
            android:gravity="center">

            <ImageButton
                android:id="@+id/ImageButton3"
                android:layout_width="@dimen/home_content_image_width"
                android:layout_height="@dimen/home_content_image_height"
                android:src="@drawable/four" />
            <TextView
                android:layout_width="@dimen/home_content_image_width"
                android:layout_height="@dimen/home_content_image_height"
                android:layout_alignBottom="@id/ImageButton3"
                android:layout_alignLeft="@id/ImageButton3"
                android:layout_alignRight="@id/ImageButton3"
                android:layout_alignTop="@id/ImageButton3"
                android:paddingBottom="@dimen/home_content_image_padding_bottom"
                android:gravity="bottom|center_horizontal"
                android:textSize="@dimen/home_text_size"
                android:textColor="@color/Home_Text_Color"
                android:fontFamily="@font/nanumsquareb"
                android:text="공학"/>
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:layout_weight="1"
            android:gravity="center">

            <ImageButton
                android:id="@+id/ImageButton4"
                android:layout_width="@dimen/home_content_image_width"
                android:layout_height="@dimen/home_content_image_height"
                android:src="@drawable/five" />
            <TextView
                android:layout_width="@dimen/home_content_image_width"
                android:layout_height="@dimen/home_content_image_height"
                android:layout_alignBottom="@id/ImageButton4"
                android:layout_alignLeft="@id/ImageButton4"
                android:layout_alignRight="@id/ImageButton4"
                android:layout_alignTop="@id/ImageButton4"
                android:paddingBottom="@dimen/home_content_image_padding_bottom"
                android:gravity="bottom|center_horizontal"
                android:textSize="@dimen/home_text_size"
                android:textColor="@color/Home_Text_Color"
                android:fontFamily="@font/nanumsquareb"
                android:text="자연"/>
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_weight="1"
            android:gravity="center">

            <ImageButton
                android:id="@+id/ImageButton5"
                android:layout_width="@dimen/home_content_image_width"
                android:layout_height="@dimen/home_content_image_height"
                android:src="@drawable/six" />
            <TextView
                android:layout_width="@dimen/home_content_image_width"
                android:layout_height="@dimen/home_content_image_height"
                android:layout_alignBottom="@id/ImageButton5"
                android:layout_alignLeft="@id/ImageButton5"
                android:layout_alignRight="@id/ImageButton5"
                android:layout_alignTop="@id/ImageButton5"
                android:paddingBottom="@dimen/home_content_image_padding_bottom"
                android:gravity="bottom|center_horizontal"
                android:textSize="@dimen/home_text_size"
                android:textColor="@color/Home_Text_Color"
                android:fontFamily="@font/nanumsquareb"
                android:text="의학"/>
        </RelativeLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <RelativeLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_weight="1"
            android:gravity="center">

            <ImageButton
                android:id="@+id/ImageButton6"
                android:layout_width="@dimen/home_content_image_width"
                android:layout_height="@dimen/home_content_image_height"
                android:src="@drawable/seven" />
            <TextView
                android:layout_width="@dimen/home_content_image_width"
                android:layout_height="@dimen/home_content_image_height"
                android:layout_alignBottom="@id/ImageButton6"
                android:layout_alignLeft="@id/ImageButton6"
                android:layout_alignRight="@id/ImageButton6"
                android:layout_alignTop="@id/ImageButton6"
                android:paddingBottom="@dimen/home_content_image_padding_bottom"
                android:gravity="bottom|center_horizontal"
                android:textSize="@dimen/home_text_size"
                android:textColor="@color/Home_Text_Color"
                android:fontFamily="@font/nanumsquareb"
                android:text="예체능"/>
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:layout_weight="1"
            android:gravity="center">

            <ImageButton
                android:id="@+id/ImageButton7"
                android:layout_width="@dimen/home_content_image_width"
                android:layout_height="@dimen/home_content_image_height"
                android:src="@drawable/seven" />
            <TextView
                android:layout_width="@dimen/home_content_image_width"
                android:layout_height="@dimen/home_content_image_height"
                android:layout_alignBottom="@id/ImageButton7"
                android:layout_alignLeft="@id/ImageButton7"
                android:layout_alignRight="@id/ImageButton7"
                android:layout_alignTop="@id/ImageButton7"
                android:paddingBottom="@dimen/home_content_image_padding_bottom"
                android:gravity="bottom|center_horizontal"
                android:textSize="@dimen/home_text_size"
                android:textColor="@color/Home_Text_Color"
                android:fontFamily="@font/nanumsquareb"
                android:text="사회"/>
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_weight="1"
            android:gravity="center">

            <ImageButton
                android:id="@+id/ImageButton8"
                android:layout_width="@dimen/home_content_image_width"
                android:layout_height="@dimen/home_content_image_height"
                android:src="@drawable/three" />
            <TextView
                android:layout_width="@dimen/home_content_image_width"
                android:layout_height="@dimen/home_content_image_height"
                android:layout_alignBottom="@id/ImageButton8"
                android:layout_alignLeft="@id/ImageButton8"
                android:layout_alignRight="@id/ImageButton8"
                android:layout_alignTop="@id/ImageButton8"
                android:paddingBottom="@dimen/home_content_image_padding_bottom"
                android:gravity="bottom|center_horizontal"
                android:textSize="@dimen/home_text_size"
                android:textColor="@color/Home_Text_Color"
                android:fontFamily="@font/nanumsquareb"
                android:text="사회"/>
        </RelativeLayout>

    </LinearLayout>

</LinearLayout>